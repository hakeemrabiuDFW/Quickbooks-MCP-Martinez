{
  "name": "quickbooks-mcp-server",
  "version": "1.0.0",
  "description": "MCP Server for QuickBooks Online integration - Martinez Cleaning LLC",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "auth": "npx tsx scripts/get-refresh-token.ts",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration/mcp-protocol.test.ts",
    "test:api": "RUN_API_TESTS=true jest tests/integration/api-real.test.ts --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:meta": "tsx tests/meta-testing/run-meta-tests.ts",
    "test:meta:chat1": "tsx tests/meta-testing/run-meta-tests.ts chat1-accountant",
    "test:meta:chat2": "tsx tests/meta-testing/run-meta-tests.ts chat2-operations",
    "test:meta:chat3": "tsx tests/meta-testing/run-meta-tests.ts chat3-owner",
    "test:meta:parallel": "npm run test:meta:chat1 & npm run test:meta:chat2 & npm run test:meta:chat3 & wait",
    "test:simple": "npx tsx tests/meta-testing/simple-test.ts"
  },
  "keywords": [
    "mcp",
    "quickbooks",
    "accounting",
    "invoices",
    "ai"
  ],
  "author": "Hakeem Rabiu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hakeemrabiuDFW/quickbooks-mcp-server"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.7.0",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.0.0",
    "jest": "^29.7.0",
    "open": "^11.0.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
